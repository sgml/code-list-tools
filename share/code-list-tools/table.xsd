<schema targetNamespace="http://wr.gatech.edu/namespace/table/"
        xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="table">
    <complexType>
      <sequence>
        <element name="row" form="qualified" minOccurs="0" maxOccurs="unbounded">
          <complexType>
            <sequence>
              <element name="cell" form="qualified" type="string" minOccurs="0" maxOccurs="unbounded"/>
            </sequence>
          </complexType>
        </element>
      </sequence>
    </complexType>
  </element>
</schema>
